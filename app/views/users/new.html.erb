<%=form_for @user do |f| %>
<%= f.label :username%>
<%= f.text_field :username%>
<%= f.label :password%>
<%= f.password_field :password%>
<%= f.label :email%>
<%= f.email_field :email%>
<%= f.label :company%>
<%= f.text_field :company%>
<%= f.label :address1%>
<%= f.text_field :address1%>
<%= f.label :address2%>
<%= f.text_field :address2%>
<%= f.label :city%>
<%= f.text_field :city%>
<%= f.label :state%>
<%= f.text_field :state%>
<%= f.label :zip%>
<%= f.text_field :zip%>
<%= f.submit "Create Account" %>
<% end %>
