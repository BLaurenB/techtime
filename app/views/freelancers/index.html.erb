<h1>Browse our Techies</h1>

<% @freelancers.each do |freelancer| %>
<div>
  <%= image_tag(freelancer.image, size: "100x100", class: "image-fl-index", id: "image_#{freelancer.id}") %>
  <h3>Name: <%= freelancer.name %></h3>
  <p><%= freelancer.description %></p>
  <p>$<%= freelancer.price %>/hr</p>
  <%= button_to "Choose Me!", cart_index_path, class: "btn btn-danger"%>
</div>
<% end %>
